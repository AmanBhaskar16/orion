generator client {
  provider = "prisma-client-js"
  output   = "../src/generated/prisma"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model UserSubmission {
  id        Int      @id @default(autoincrement())
  email     String   @unique
  role      String
  createdAt DateTime @default(now())
}
